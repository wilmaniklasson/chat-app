{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "ESNext",
	  "moduleResolution": "node", // Lägg till detta
	  "outDir": "./backendDist", // Sätt utmatningskatalogen här
	  "rootDir": "./backendSrc", // Sätt rotkatalogen här
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["backendSrc/**/*"], // Inkludera alla filer i backendSrc
	"exclude": ["node_modules", "**/*.spec.ts"] // Exkludera oönskade filer
  }
  